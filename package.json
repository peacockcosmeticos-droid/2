  "description": "",
  "devDependencies": {
    "@fontsource/inter": "^5.2.6",
    "@fontsource/poppins": "^5.2.6",
    "@playwright/test": "^1.55.0",
    "autoprefixer": "^10.4.21",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.3",
    "svgo": "^4.0.0",
    "swiper": "^11.2.10",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@mux/mux-node": "^12.7.0",
    "@mux/mux-player": "^3.5.3",
    "@mux/mux-uploader": "^1.2.0"
  }
